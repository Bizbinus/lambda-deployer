{
  "name": "deploy",
  "version": "1.0.0",
  "description": "deploys all functions in lambda folder to aws lambda",
  "main": "deploy.js",
  "scripts": {
    "test": "mocha ./**/*.spec.js",
    "deploy-dev": "node -r dotenv/config deploy.js dotenv_config_path=./config/env/dev/.env",
    "deploy-staging": "node -r dotenv/config deploy.js  dotenv_config_path=./config/env/staging/.env",
    "deploy-prod": "node -r dotenv/config deploy.js dotenv_config_path=./config/env/production/.env"
  },
  "author": "",
  "license": "ISC", 
  "dependencies": {
    "archiver": "^2.1.1",
    "aws": "0.0.3-2",
    "aws-sdk": "^2.194.0",
    "dotenv": "^6.1.0",
    "node-stream-zip": "^1.7.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "memory-fs": "^0.4.1",
    "mocha": "^5.2.0",
    "mock-fs": "^4.7.0",
    "webpack": "^4.25.1"
  }
}
